{
  "multipart": [
    {   "when": { "OR": [
      {"north": "none", "east": "none", "south": "none", "west": "none"},
      {"north": "side|up", "east": "side|up" },
      {"east": "side|up", "south": "side|up" },
      {"south": "side|up", "west": "side|up"},
      {"west": "side|up", "north": "side|up"}
    ]},
      "apply": { "model": "quaritum:chalkDot" }
    },
    {   "when": { "OR": [
      { "north": "side|up" },
      { "north": "none", "east": "none", "south": "side|up", "west": "none" }
    ]},
      "apply": { "model": "quaritum:chalkSide0" }
    },
    {   "when": { "OR": [
      { "south": "side|up" },
      { "north": "side|up", "east": "none", "south": "none", "west": "none" }
    ]},
      "apply": { "model": "quaritum:chalkSideAlt0" }
    },
    {   "when": { "OR": [
      { "east": "side|up" },
      { "north": "none", "east": "none", "south": "none", "west": "side|up" }
    ]},
      "apply": { "model": "quaritum:chalkSideAlt1", "y": 270 }
    },
    {   "when": { "OR": [
      { "west": "side|up" },
      { "north": "none", "east": "side|up", "south": "none", "west": "none" }
    ]},
      "apply": { "model": "quaritum:chalkSide1", "y": 270 }
    },
    {   "when": { "north": "up" },
      "apply": { "model": "quaritum:chalkUp" }
    },
    {   "when": { "east": "up" },
      "apply": { "model": "quaritum:chalkUp", "y": 90 }
    },
    {   "when": { "south": "up" },
      "apply": { "model": "quaritum:chalkUp", "y": 180 }
    },
    {   "when": { "west": "up" },
      "apply": { "model": "quaritum:chalkUp", "y": 270 }
    }
  ]
}
