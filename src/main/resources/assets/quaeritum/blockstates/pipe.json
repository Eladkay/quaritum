{
    "multipart": [
        { "when": { "down":  "false" }, "apply": { "model": "quaeritum:pipe_none", "x": 90,  "uvlock": true } },
        { "when": { "down":  "true"  }, "apply": { "model": "quaeritum:pipe_side", "x": 90,  "uvlock": true } },
        { "when": { "up":    "false" }, "apply": { "model": "quaeritum:pipe_none", "x": 270, "uvlock": true } },
        { "when": { "up":    "true"  }, "apply": { "model": "quaeritum:pipe_side", "x": 270, "uvlock": true } },
        { "when": { "north": "false" }, "apply": { "model": "quaeritum:pipe_none" } },
        { "when": { "north": "true"  }, "apply": { "model": "quaeritum:pipe_side" } },
        { "when": { "south": "false" }, "apply": { "model": "quaeritum:pipe_none", "y": 180, "uvlock": true } },
        { "when": { "south": "true"  }, "apply": { "model": "quaeritum:pipe_side", "y": 180, "uvlock": true } },
        { "when": { "west":  "false" }, "apply": { "model": "quaeritum:pipe_none", "y": 270, "uvlock": true } },
        { "when": { "west":  "true"  }, "apply": { "model": "quaeritum:pipe_side", "y": 270, "uvlock": true } },
        { "when": { "east":  "false" }, "apply": { "model": "quaeritum:pipe_none", "y": 90,  "uvlock": true } },
        { "when": { "east":  "true"  }, "apply": { "model": "quaeritum:pipe_side", "y": 90,  "uvlock": true } }
    ]
}